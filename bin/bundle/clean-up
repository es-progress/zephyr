#!/usr/bin/env bash
#############################
## ES-Ubuntu               ##
##                         ##
## Bundling                ##
## Clean-up unpacked files ##
#############################

# Strict mode
set -euo pipefail
IFS=$'\n\t'

source "${PROJECT_ROOT}/bootstrap.sh"
check-not-root || exit 1

print-status "Remove extracted files..."
sudo rm -rf "${ES_EXTRACT}"
print-finish

exit 0
