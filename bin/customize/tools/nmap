#!/usr/bin/env bash
###############
## ES-Ubuntu ##
##           ##
## Packages  ##
## nmap      ##
###############

# Strict mode
set -euo pipefail
IFS=$'\n\t'

source "${PROJECT_ROOT}/bootstrap.sh"
check-not-root || exit 1

print-header "Install plugins..."
sudo snap connect nmap:network-control
print-finish

exit 0
