#!/usr/bin/env bash
###############
## ES-Ubuntu ##
##           ##
## Config    ##
## GNOME     ##
## Terminal  ##
###############

# Strict mode
set -euo pipefail
IFS=$'\n\t'

source "${ESUBUNTU_HOME}/bootstrap.sh"
check-not-root || exit 1

print-status "Config GNOME terminal..."
sed -e '/^#/d' -e '/^$/d' "${ES_CFG_CUSTOMIZE}/gnome/terminal" | dconf load /org/gnome/terminal/
print-finish

exit 0
