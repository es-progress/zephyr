#!/usr/bin/env bash
###############
## ES-Ubuntu ##
##           ##
## Config    ##
## GNOME     ##
## Autostart ##
###############

# Strict mode
set -euo pipefail
IFS=$'\n\t'

source "${DIR_ESUBUNTU}/bootstrap.sh"
check-not-root || exit 1

print-status "Copy autostart files..."
mkdir -p ~/.config/autostart/
cp -a "${ES_CUSTOMIZE_CFG}"/gnome/autostart.d/* ~/.config/autostart
print-finish

exit 0
