#!/usr/bin/env bash
#############################
## Zephyr                  ##
##                         ##
## Remix                   ##
## Clean-up unpacked files ##
#############################

# Strict mode
set -eufo pipefail
IFS=$'\n\t'

source "${PROJECT_ROOT}/bin/bootstrap.sh"
check-not-root

print-status "Remove extracted files..."
sudo rm -rf "${PATH_EXTRACT}"
print-finish

print-status "Remove ISO mount point..."
sudo rm -rf "${PATH_MOUNT}"
print-finish

exit 0
